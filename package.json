{
  "name": "oh-my-boilingpoint",
  "version": "0.0.1",
  "description": "Oh gee, some (yet another) boiler plate code for my preferred React setup (React, TS, Storyboard, Jest, Linting, all with Webpack and Babel and base configs)...",
  "homepage": "https://github.com/spxrogers/oh-my-boilingpoint#readme",
  "bugs": {
    "url": "https://github.com/spxrogers/oh-my-boilingpoint/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spxrogers/oh-my-boilingpoint.git"
  },
  "license": "MIT",
  "author": "steven rogers",
  "main": "src/index.tsx",
  "scripts": {
    "build": "webpack --mode production --config ./webpack.config.js",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx} --quiet --fix --no-error-on-unmatched-pattern",
    "sortPackageJson": "npx sort-package-json",
    "storybook:build": "build-storybook",
    "storybook:serve": "start-storybook -p 6006",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "types:check": "tsc",
    "types:watch": "tsc --watch"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-essentials": "^6.1.11",
    "@storybook/addon-links": "^6.1.11",
    "@storybook/react": "^6.1.11",
    "@testing-library/react": "^11.2.2",
    "@types/jest": "^26.0.19",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "html-webpack-plugin": "^5.0.0-beta.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.1",
    "typescript": "^4.1.3",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.0"
  }
}
